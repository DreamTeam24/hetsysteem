{{#block "title"}}Het Systeem - Inventaris{{/block}}

<h1>Inventaris</h1>
<h2>Items</h2>
{{#if items.length}}
<ul>
{{#each items}}
	<li>{{name}}: {{quantity}}</li>
{{/each}}
</ul>
{{else}}
<p>Je hebt geen items.</p>
{{/if}}
<h2>Gebouwen</h2>
{{#if buildings.length}}
<ul>
{{#each buildings}}
	<li>{{name}}: {{quantity}}</li>
{{/each}}
</ul>
{{else}}
<p>Je hebt geen gebouwen.</p>
{{/if}}
<div class="mw-container">
	<a href="/game/play" class="button-2">Terug</a>
</div>